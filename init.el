 ;; Add Packages
(defvar my/packages '(
		      evil
		      smex
		      company
		      smartparens
		      evil-leader
		      helm
		      org-pomodoro
                      htmlize
                      evil-nerd-commenter
                      window-numbering
                      powershell
                      sound-wav
                      solarized-theme
                      hungry-delete
                      monokai-theme
                      projectile
                      neotree
                      helm-gtags
                      ggtags
                      function-args
                      sr-speedbar
                      smooth-scrolling
                      popwin
                      powerline
                      powerline-evil
                      nyan-mode
                      which-key
                      expand-region
                      dracula-theme
                      color-theme-sanityinc-tomorrow
                      rainbow-delimiters
                      aggressive-indent 
                      anaconda-mode
                      ) "Default packages")

 (setq package-selected-packages my/packages)

 (defun my/packages-installed-p ()
     (loop for pkg in my/packages
           when (not (package-installed-p pkg)) do (return nil)
           finally (return t)))

 (unless (my/packages-installed-p)
     (message "%s" "Refreshing package database...")
     (package-refresh-contents)
     (dolist (pkg my/packages)
       (when (not (package-installed-p pkg))
         (package-install pkg))))

